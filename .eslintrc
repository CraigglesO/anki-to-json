{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": ["error", "single"],
    "comma-style": ["error", "last", { "exceptions": { "ArrayExpression": true, "ObjectExpression": true } }],
    "comma-spacing": ["error", { "before": false, "after": true }]
  }
}
